NotaEPG3308 <- function(p, pMax, Ex){
if(p < 0) stop("¡No se calculan notas negativas!")
if(p < Ex * pMax){
Nota <- 1 + 3*p/(Ex*pMax)
return( round(Nota,1) )
}else{ # En caso contrario, p >= Ex * pMax
Nota <- 4 + 3*(p - Ex*pMax)/(pMax*(1- Ex))
return( round(Nota,1) )
}
}
NotaEPG3308(p = 9, pMax = 13, Ex = 0.6)
